platform :ios, '11.0'
use_frameworks!

target 'ChatRobot' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks

  # Pods for ChatRobot
  
		# Rx
		pod 'RxSwift'		# RxSwift的必备库
		pod 'RxCocoa'		# 对 UIKit Foundation 进行 Rx 化 - 本次未使用 - BehaviorRelay
		#pod 'RxDataSources'		# 帮助我们优雅的使用tableView的数据源方法 - 本次未使用
		
		# 网络请求
		pod 'Moya'
		pod 'Moya/RxSwift'		# 为RxSwift专用提供，对Alamofire进行封装的一个网络请求库
		pod 'HandyJSON'				# 网络返回数据json解析
		
		# JSON
		pod 'SwiftyJSON', '~> 4.0'		# socket返回json数据解析

end
